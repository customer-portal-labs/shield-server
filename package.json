{
  "name": "@cplabs/shield-server",
  "version": "0.0.5",
  "description": "",
  "bin": "./dist/bin/shield-server.js",
  "main": "./dist/index.js",
  "types": "./@types/index.d.ts",
  "files": [
    "dist",
    "@types"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "dev": "ts-node ./src/bin/shield-server.ts",
    "build": "tsc --build tsconfig.production.json",
    "pretest": "npm run lint",
    "test": "nyc mocha",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf ./dist ./@types ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/customer-portal-labs/shield-server.git"
  },
  "author": "Kun Yan <kyan@redhat.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/customer-portal-labs/shield-server/issues"
  },
  "homepage": "https://github.com/customer-portal-labs/shield-server#readme",
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/chai": "4.2.14",
    "@types/compression": "1.7.0",
    "@types/connect-history-api-fallback": "1.3.3",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.9",
    "@types/express": "4.17.11",
    "@types/figlet": "1.2.1",
    "@types/lodash": "4.14.168",
    "@types/mocha": "8.2.0",
    "@types/morgan": "1.9.2",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "chai": "4.2.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.1.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "supertest": "6.1.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "arg": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cosmiconfig": "^7.0.0",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "helmet": "^4.2.0",
    "http-proxy-middleware": "^1.0.6",
    "internal-ip": "^6.2.0",
    "lodash": "^4.17.20",
    "log2splunk": "0.0.4",
    "morgan": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write"
    ]
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": "test/**/*.spec.ts",
    "require": "ts-node/register"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": "lcov"
  }
}
